{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://anoduck.github.io/timelinejs-json-schema/timelinejs.media.schema.json",
  "title": "Media",
  "description": "A media object for timelinejs",
	"type": "object",
	"properties": {
		"url": {
			"type": "string"
		},
		"caption": {
			"type": "string"
		},
		"credit": {
			"type": "string"
		},
		"thumbnail": {
			"type": "string"
		},
		"alt": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"link": {
			"type": "string"
		},
		"link_target": {
			"type": "string"
		}
	},
	"required": [ "url" ]
}
